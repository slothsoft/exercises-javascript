{
  "name": "exercises-javascript",
  "version": "1.0.0",
  "description": "A couple of programming exercises I thought would be fun to try.",
  "scripts": {
    "bundle": "browserify src/index.js | squash > dist/resources/js/application.js && npm run copy-exercises",
    "copy-exercises": "copyfiles --up 1 src/exercise/*.js dist/",
    "watch": "watchify src/index.js -o dist/resources/js/application.js -v",
    "test": "qunit && npm run generate-test-report",
    "generate-readme": "node script/generate-readme.js",
    "generate-test-report": "node script/generate-test-report.js",
    "create-new-exercise": "node script/create-new-exercise.js",
    "deploy": "npm run test && npm run bundle && npm run generate-readme && node script/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slothsoft/exercises-javascript"
  },
  "keywords": [
    "example",
    "html",
    "javascript",
    "website"
  ],
  "author": "Stef Schulz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slothsoft/exercises-javascript/issues"
  },
  "homepage": "https://github.com/slothsoft/exercises-javascript/blob/master/README.md",
  "devDependencies": {
    "butternut": "^0.4.6",
    "copyfiles": "^2.1.1",
    "ftp-deploy": "^2.3.3",
    "qunit": "^2.9.2",
    "qunit-reporter-junit": "^1.1.1",
    "watchify": "^3.11.1",
    "xslt-processor": "^0.11.5"
  },
  "dependencies": {}
}
